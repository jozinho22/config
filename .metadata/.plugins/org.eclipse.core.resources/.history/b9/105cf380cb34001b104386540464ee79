package com.douineau.testspringboot.util;

import javax.servlet.http.HttpServletRequest;

public class ApiUtil {
	
	private static Class<?> getClassNameWithRequest(HttpServletRequest request) {
		int thirdSlashPlace = request.getRequestURI().lastIndexOf("/");
		String key = request.getRequestURI().substring(0, thirdSlashPlace);
		Class<?> clazz = apiMapping.get(key);
		System.out.println(request.getRequestURI());
		return clazz;
	}

}
