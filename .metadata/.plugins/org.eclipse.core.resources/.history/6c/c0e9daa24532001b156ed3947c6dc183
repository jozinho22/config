package com.douineau.testspringboot.dao.security;

import java.util.Optional;

import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.douineau.testspringboot.security.ApplicationUserDetails;

@Repository
public interface UserDao extends CrudRepository<ApplicationUserDetails, Integer> {

	Optional<ApplicationUserDetails> findByName(String name);

}
